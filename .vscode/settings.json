{
  "terminal.integrated.profiles.linux": {
    "bash": {
      "path": "bash"
    }
  },
  "elmLS.elmFormatPath": "./node_modules/.bin/elm-format",
  "elmLS.elmTestPath": "./node_modules/.bin/elm-test",
  "elmLS.elmReviewDiagnostics": "error",
  "elmLS.elmReviewPath": "./node_modules/.bin/elm-review",
  "elmLS.disableElmLSDiagnostics": true,
  "editor.tabSize": 4,
  "editor.formatOnSave": true,
  "editor.linkedEditing": false,
  "editor.defaultFormatter": "elmTooling.elm-ls-vscode",
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "files.insertFinalNewline": true,
  "files.eol": "\n",
  "editor.codeActionsOnSave": {
    "source.fixAll": true,
    "source.fixAll.eslint": false
  },
  "eslint.run": "onSave",
  "eslint.workingDirectories": ["./end-to-end-tests"],
  "typescript.tsdk": "./end-to-end-tests/node_modules/typescript/lib",
  // Javascript stuff
  "prettier.prettierPath": "./end-to-end-tests/node_modules/prettier",
  "prettier.ignorePath": "./end-to-end-tests/.prettierignore",
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json5]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}
